PROGRAM Test IS

  INSTRUCTION one IS
     move
     turnleft
  END one
  
  INSTRUCTION two IS
     IF next-is-enemy THEN
        one
        turnright
        move
     END IF
  END two
  
  INSTRUCTION three IS
    one
    IF next-is-not-empty THEN
      turnleft
    ELSE
      one
      one
    END IF
  END three

BEGIN
  infect
  WHILE true DO
    two
    IF next-is-empty THEN
      move
    END IF
    two
    one
  END WHILE
END Test